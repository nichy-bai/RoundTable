                    <div class="flex flex-col hidden lg:inline">
                        <!--Featured discussions-->
                        <div
                            class="m-5 mt-0 ml-0 items-center justify-between bg-white w-96 h-auto px-5 py-4 rounded-lg shadow-md transition ease-in-out duration-1000">
                            <div class="flex flex-row items-center text-lg font-semibold text-gray-800 hover:text-indigo-600 mb-2 pb-2 border-b-2 border-gray-300 hover:border-indigo-500 transition ease-in-out duration-300 cursor-default">
